		</property>
		<property name="SlotTypeValue">
			<column name="SlotTypeValue" sql-type="int(11)" not-null="true" />
		</property>
		<joined-subclass table="EquipmentTypeAmmo"	name="EquipmentTypeAmmo" >
			<key column="RefID"/>
			<property name="AmmoTypeValue">
				<column name="AmmoTypeValue" sql-type="int(11)" not-null="true" />
			</property>
			<property name="CaliberTypeValue">
				<column name="CaliberTypeValue" sql-type="int(11)" not-null="true" />
			</property>
			<property name="DamageModArmor">
