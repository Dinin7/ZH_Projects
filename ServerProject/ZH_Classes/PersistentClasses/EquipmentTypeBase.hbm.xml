			</property>
			<property name="DamageMod">
				<column name="DamageMod" sql-type="int(11)" not-null="true" />
			</property>
		</joined-subclass>
		<joined-subclass table="EquipmentTypeArmor"	name="EquipmentTypeArmor" >
			<key column="RefID"/>
			<property name="DamageModBullet">
				<column name="DamageMod_Bullet" sql-type="int(11)" not-null="true" />
			</property>
			<property name="DamageModHit">
				<column name="DamageMod_Hit" sql-type="int(11)" not-null="true" />
			</property>
			<property name="ConditionLossPerBullet">
				<column name="ConditionLossPerBullet" sql-type="decimal(10,0)" not-null="true" />
			</property>
			<property name="ConditionLossPerHit">
				<column name="ConditionLossPerHit" sql-type="decimal(10,0)" not-null="true" />
			</property>
		</joined-subclass>
		<joined-subclass table="EquipmentTypeWeapon"	name="EquipmentTypeWeapon" >
			<key column="RefID"/>
			<property name="CaliberTypeValue">
				<column name="CaliberTypeValue" sql-type="int(11)" not-null="true" />
			</property>
			<property name="WeaponRange">
				<column name="WeaponRange" sql-type="int(11)" not-null="true" />
			</property>
			<property name="PrecisionManual">
				<column name="PrecisionManual" sql-type="int(11)" not-null="false" />
			</property>
			<property name="PrecisionAuto">
				<column name="PrecisionAuto" sql-type="int(11)" not-null="false" />
			</property>
			<property name="Damage">
