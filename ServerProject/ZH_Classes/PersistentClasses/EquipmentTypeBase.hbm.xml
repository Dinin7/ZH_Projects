				<column name="Spreading" sql-type="int(11)" not-null="true" />
			</property>
			<property name="DamageModArmor">
				<column name="DamageModArmor" sql-type="int(11)" not-null="true" />
			</property>
		</joined-subclass>
	</class>
</hibernate-mapping>