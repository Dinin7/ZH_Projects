<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ZH_Classes" namespace="ZH_Classes" xmlns="urn:nhibernate-mapping-2.2">
	<class name="EquipmentTypeBase" table="EquipmentTypeBase" lazy="true" >
		<id name="Id">
			<generator class="identity" />
		</id>
		<property name="EqName">
			<column name="EqName" sql-type="varchar(100)" not-null="true" />
		</property>
		<property name="ReqSkillId">
			<column name="ReqSkillId" sql-type="int(11)" not-null="false" />
		</property>
		<property name="Price">
			<column name="Price" sql-type="int(11)" not-null="true" />
		</property>
		<property name="Weight">
			<column name="Weight" sql-type="int(11)" not-null="true" />
		</property>
		<property name="ReqRelationPoints">
			<column name="ReqRelationPoints" sql-type="int(11)" not-null="false" />
