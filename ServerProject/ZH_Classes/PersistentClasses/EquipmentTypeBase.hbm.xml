		<property name="ZombieAttraction">
			<column name="ZombieAttraction" sql-type="int(11)" not-null="true" />
		</property>
		<property name="ObjectType">
			<column name="ObjectType" sql-type="int(11)" not-null="true" />
		</property>
		<property name="Complexity">
			<column name="Complexity" sql-type="int(11)" not-null="true" />
		</property>
		<property name="AmountSlots">
