			</property>
			<property name="ConditionLossPerBullet">
				<column name="ConditionLossPerBullet" sql-type="decimal(10,0)" not-null="true" />
			</property>
			<property name="AutomaticTypeValue">
				<column name="AutomaticTypeValue" sql-type="int(11)" not-null="true" />
			</property>
			<property name="FirerateManual">
				<column name="FirerateManual" sql-type="decimal(10,0)" not-null="false" />
			</property>
			<property name="FirerateAuto">
				<column name="FirerateAuto" sql-type="decimal(10,0)" not-null="false" />
