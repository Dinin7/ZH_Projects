<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ZH_Classes" namespace="ZH_Classes" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Accuracy" table="Accuracy" lazy="true" >
    <id name="Id">
      <generator class="identity" />
    </id>
    <property name="CharacterID">
      <column name="CharacterID" sql-type="int(11)" not-null="true" />
    </property>
    <property name="AccuracyValue">
      <column name="AccuracyValue" sql-type="int(11)" not-null="true" />
    </property>
    <property name="WeaponTypeValue">
      <column name="WeaponTypeValue" sql-type="int(11)" not-null="true" />
    </property>
  </class>
</hibernate-mapping>